<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.mainview.MainViewController">
    <left>
        <AnchorPane fx:id="menuPane" prefHeight="600.0" prefWidth="130.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" />
            </padding>
        </AnchorPane>
    </left>
    <center>
        <AnchorPane fx:id="rightArea" prefHeight="600.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <opaqueInsets>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </opaqueInsets>
        </AnchorPane>
    </center>
</BorderPane>
